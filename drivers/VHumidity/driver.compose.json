{
  "id": "VHumidity",
  "name": {
    "en": "VHumidity"
  },
  "icon": "{{driverAssetsPath}}/icon.svg",
  "images": {
    "large": "{{driverAssetsPath}}/images/large.png",
    "small": "{{driverAssetsPath}}/images/small.png"
  },
  "class": "thermostat",
  "capabilities": [
    "vt_onoff",
    "vh_target_humidity",
    "measure_humidity"
  ],
  "capabilitiesOptions": {
    "vt_onoff": {
      "greyout": true
    },
    "vh_target_humidity": {
      "decimals": 0,
      "min": 1,
      "max": 90,
      "step": 1
    }
  },
  "pair": [
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_my_devices"
      }
    },
    {
      "id": "add_my_devices",
      "template": "add_devices"
    }
  ]
}